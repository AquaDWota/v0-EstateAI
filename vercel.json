{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install && pip install -r backend/requirements.txt",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/analyze-properties",
      "destination": "/api/backend/analyze-properties"
    },
    {
      "source": "/api/agent-commentary",
      "destination": "/api/backend/agent-commentary"
    },
    {
      "source": "/api/properties/:zip_code",
      "destination": "/api/backend/properties/:zip_code"
    }
  ],
  "functions": {
    "api/backend/*.py": {
      "runtime": "python3.9"
    }
  }
}
